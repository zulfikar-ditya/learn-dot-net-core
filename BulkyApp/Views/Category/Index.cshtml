@model List<Category>

<h1>Category</h1>
<table class="table table-responsive table-stripped">
    <thead>
        <tr>
    <th>Name</th>
    <th>Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>@obj.Name</td>
                <td>@obj.DisplayOrder</td>
            </tr>
        }
    </tbody>
</table>